  <system.net>
    <defaultProxy enabled="true">      
      <proxy proxyaddress="http://127.0.0.1:8580"  />
    </defaultProxy>
    <mailSettings>
      <smtp from="no-reply@kooboo.com">
        <network host="127.0.0.1"/>
      </smtp>
    </mailSettings>
