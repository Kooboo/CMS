<?xml version="1.0" encoding="utf-8"?>
<ServiceDefinition name="Kooboo.CMS.Azure" xmlns="http://schemas.microsoft.com/ServiceHosting/2008/10/ServiceDefinition">
  <WebRole name="Kooboo.CMS.Web" vmsize="Small">
    <Sites>
      <Site name="Web" physicalDirectory="Kooboo_CMS">
        <Bindings>
          <Binding name="Endpoint1" endpointName="Endpoint1" />
        </Bindings>
      </Site>
    </Sites>
	<Startup>
      <Task commandLine="..\Startup.bat" executionContext="elevated" taskType="simple" />
    </Startup>
    <Endpoints>
      <InternalEndpoint name="InternalHttpIn" protocol="http"/>
      <InputEndpoint name="Endpoint1" protocol="http" port="80" localPort="80" />
    </Endpoints>
    <Imports>
      <Import moduleName="Diagnostics" />
      <Import moduleName="RemoteAccess" />
      <Import moduleName="RemoteForwarder" />
    </Imports>
    <LocalResources>
    </LocalResources>
  </WebRole>
</ServiceDefinition>